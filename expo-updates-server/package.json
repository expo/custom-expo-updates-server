{
  "name": "custom-expo-updates-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "PRIVATE_KEY_PATH=code-signing-keys/private-key.pem next dev --turbopack",
    "build": "next build",
    "start": "PRIVATE_KEY_PATH=code-signing-keys/private-key.pem next start",
    "lint": "eslint common pages",
    "test": "jest",
    "expo-publish": "./scripts/publish.sh -d 2/$(date +%s)",
    "expo-publish-rollback": "mktouch() { mkdir -p $(dirname $1) && touch $1; }; cd ../expo-updates-server && mktouch updates/1/$(date +%s)/rollback",
    "expo-publish-test": "./scripts/publish.sh -d test/$(date +%s)"
  },
  "dependencies": {
    "@expo/spawn-async": "^1.7.2",
    "form-data": "^4.0.4",
    "mime": "^2.5.2",
    "next": "15.5.3",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "structured-headers": "^0.4.1"
  },
  "devDependencies": {
    "@expo/config": "^12.0.9",
    "@expo/multipart-body-parser": "^2.0.0",
    "@types/jest": "^30.0.0",
    "@types/mime": "^2.0.3",
    "@types/node": "^24.5.1",
    "@types/react": "19.1.13",
    "eslint": "^9.35.0",
    "eslint-config-universe": "^15.0.3",
    "jest": "^30.1.3",
    "node-mocks-http": "^1.17.2",
    "nullthrows": "^1.1.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  },
  "resolutions": {
    "@types/react": "19.1.13"
  }
}
